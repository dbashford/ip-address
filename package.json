    "ipv6",
    "ipv4",
    "browser",
    "validation"
  ],
  "version": "6.1.0",
  "author": "Beau Gunderson <beau@beaugunderson.com> (https://beaugunderson.com/)",
  "license": "MIT",
  "main": "lib/ip-address.js",
  "module": "es/ip-address.mjs",
  "scripts": {
