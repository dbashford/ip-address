    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "browserify": "^16.3.0",
    "chai": "^4.2.0",
    "codecov.io": "^0.1.6",
    "documentation": "^12.0.3",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.0",
    "mochify": "^6.3.0"
